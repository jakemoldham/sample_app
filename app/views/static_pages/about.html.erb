        <h5>2 - 3 HOURS <br> PER DAY</h5>
      </div>                        <!--end league-->

      <div class="contact">
        <div id="header_contact">
          <h2>CONTACT</h2>
          <div class="line"></div>
          <p>Want to work together? Or just drop a line? Just say hi!</p>
        </div>
            <!-- <form> -->
              
                <div id="one">
                  <!-- <h6 id="name">NAME</h6> -->
                  <p><input type="text" id="namebox"placeholder="NAME&hellip;"></p>
                </div>                          <!--end name-->
                <div id="two">
                  <!-- <h6 id="email">EMAIL-ADDRESS</h6> -->
                  <p><input type="text" id="emailbox" placeholder="EMAIL&hellip;"></p>  
                </div>                          <!--end email-->
                <div id="three">    
                  <!-- <p class="message">MESSAGE</p> -->
                  <textarea id="message" placeholder="MESSAGE HERE&hellip;"></textarea>
                  <button type="Submit" class="submit"><p>Submit</p></button>
                </div>                          <!--end message-->
            <!-- </form> -->
        </div>                                <!--end contact-->
      <footer>
            <p>&copy; 2015 Jake Oldham.</p>
      </footer>   
  </div>                            <!--end body div-->
