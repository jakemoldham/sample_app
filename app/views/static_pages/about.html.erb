<% provide(:title, "About") %>
<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>Jake Oldham  |  Programmer </title>
  <link rel="stylesheet" href="/about_me.css.scss"> 
</head>
  <body>
  <div> 
    <div class="navigator">
      <nav>
        <h3> ABOUT ME </h3>
      </nav>


    </div>                                                    <!--end nav-->
      <div class="hero">
        <%= image_tag "Portrait.png" %>
        <br>
        <h1>JACOB OLDHAM</h1>
        <h3>DEVELOPER | STUDENT</h3>
      </div>                                                <!--end hero-->

      <div class="drpepper">
        <h2>DR PEPPER <br> CONSUMPTION </h2>
        <div class="line"></div>
        <h5>3-4 CANS <br> PER DAY</h5>
      </div>                                             <!---end dr pepper-->

      <div class="league">
